{
    "name": "wirasakti/iris",
    "description": "Laravel vendor untuk pembayaran keluar via midtrans iris",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "lucky wirasakti",
            "email": "lucky.wirasakti@icloud.com"
        }
    ],
    "require": {
        "illuminate/support": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Wirasakti\\Iris\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wirasakti\\Iris\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wirasakti\\Iris\\IrisServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
